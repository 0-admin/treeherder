#!/bin/bash
  curr_dir=$( dirname "${BASH_SOURCE[0]}" )
  cd $( dirname $curr_dir)
  source /etc/profile.d/treeherder.sh
  source ../venv/bin/activate
  exec ../venv/bin/python manage.py celeryd -Q log_parser_fail,log_parser -P gevent --concurrency=10 -E --logfile=/var/log/celery/treeherder_worker_gevent.log -n gevent_worker.%h -l info --maxtasksperchild=500
